{
	"extends": ["airbnb-base", "prettier"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"end-of-line": "auto"
			}
		],
		"no-console": "off",
		"no-unused-vars": "warn",
		"func-names": "off",
		"object-shorthand": "off",
		"no-shadow": "off",
		"spaced-comment": [2, "always"],
		"consistent-return": "off",
		"no-underscore-dangle": "off",
		"radix": "off",
		"no-param-reassign": "off",
		"no-nested-ternary": "off",
		"no-plusplus": "off"
	}
}
